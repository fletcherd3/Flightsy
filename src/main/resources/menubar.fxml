<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="37.0" prefWidth="750.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team6.gui.controller.MenubarController">
    <Pane styleClass="h3">
      <Label alignment="CENTER" prefHeight="35.0" prefWidth="120.0" text="Flightsy" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <styleClass>
            <String fx:value="lbl" />
            <String fx:value="lbl-primary" />
         </styleClass>
      </Label>
    </Pane>
    <Pane prefWidth="122.0" visible="false" HBox.hgrow="ALWAYS" />
    <Button fx:id="flightPlannerButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="35.0" prefWidth="135.0" text="Flight Planner" textAlignment="CENTER">
    <styleClass>
       <String fx:value="btn" />
       <String fx:value="btn-primary" />
    </styleClass>
    </Button>
    <Button fx:id="tripsButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="35.0" prefWidth="135.0" text="Trips" textAlignment="CENTER">
    <styleClass>
       <String fx:value="btn" />
       <String fx:value="btn-primary" />
    </styleClass>
    </Button>
    <Button fx:id="dataButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="35.0" prefWidth="135.0" text="Data" textAlignment="CENTER">
    <styleClass>
       <String fx:value="btn" />
       <String fx:value="btn-primary" />
    </styleClass>
    </Button>
</HBox>
